@model List<CarInsurance.Models.Insuree>

@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

<table>
    <thead>
        <tr>
            <th>First Name </th>
            <th>Last Name </th>
            <th>Email </th>
            <th>Quote </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var insuree in ViewBag.Insurees)
        {
            <tr>
                <td>@insuree.FirstName </td>
                <td>@insuree.LastName </td>
                <td>@insuree.EmailAddress </td>
                <td>@ViewBag.Quotes[insuree.Id]</td>
            </tr>
        }
    </tbody>
</table>
